<% if users.any? %>
  <h5><%= "#{header} (#{users.count})" %></h5>
  <table>
    <tr>
      <th></th>
      <th></th>
      <% if @is_admin %>
        <th>% Present</th>
        <th>% Complete</th>
        <th>Average Obz</th>
        <th></th>
        <th></th>
      <% end %>
    </tr>
    <% users.each do |user| %>
      <%= render partial: "user_row", locals: {user: user} %>
    <% end %>
  </table>
<% else %>
<% end  %>
