<% if memberships.any? %>
  <h5><%= "#{header} (#{memberships.count})" %></h5>
  <table>
    <tr>
      <th></th>
      <th></th>
      <% if current_user.is_admin_of_anything? %>
        <th>% Present</th>
        <th>% Complete</th>
        <th>Average Obz</th>
        <th></th>
        <th></th>
      <% end %>
    </tr>
    <% memberships.each do |membership| %>
      <%= render partial: "memberships/row", locals: {membership: membership, row_header: row_header} %>
    <% end %>
  </table>
<% else %>
<% end  %>
